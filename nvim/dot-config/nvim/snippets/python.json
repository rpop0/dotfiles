{
    "docstring-simple": {
        "prefix": "dss",
        "body": [
            "\"\"\"${1:Summarize the function in one line.}",
            "",
            "${2:Extended description, only clarify functionality and not implementation. Ref variables in `backticks`}",
            "\"\"\""
        ]
    },
    "docstring": {
        "prefix": "ds",
        "body": [
            "\"\"\"${1:Summarize the function in one line.}",
            "",
            "${2:Extended description, only clarify functionality and not implementation. Ref variables in `backticks`}",
            "",
            "Parameters",
            "----------",
            "${3:parameter_name} : ${4:parameter type, [optional/default]}",
            "   ${5:Parameter description}",
            "\"\"\""
        ]
    },
    "docstring-returns": {
        "prefix": "dsr",
        "body": [
            "\"\"\"${1:Summarize the function in one line.}",
            "",
            "${2:Extended description, only clarify functionality and not implementation. Ref variables in `backticks`}",
            "",
            "Parameters",
            "----------",
            "${3:parameter_name} : ${4:parameter type, [optional/default]}",
            "   ${5:Parameter description}",
            "",
            "Returns",
            "-------",
            "${6:type}",
            "   ${7:Explanation of return value.}",
            "\"\"\""
        ]
    },
    "docstring-property": {
        "prefix": "dsp",
        "body": [
            "\"\"\"${1:type}: ${2:Only document properties in getter. Mention setter functionality if exists here}\"\"\""
        ]
    }
}
